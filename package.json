{
  "name": "voice-ai-agent",
  "version": "1.0.0",
  "description": "A comprehensive AI voice agent with real-time speech processing",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "start": "node server/index.js"
  },
  "keywords": [
    "ai",
    "voice",
    "speech",
    "agent",
    "real-time"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "axios": "^1.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "multer": "^1.4.5-lts.1",
    "node-record-lpcm16": "^1.0.1",
    "ollama": "^0.4.4",
    "openai": "^4.20.1",
    "play": "^0.5.0",
    "socket.io": "^4.7.2",
    "speaker": "^0.5.4",
    "uuid": "^9.0.0",
    "wav": "^1.0.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1"
  }
}
